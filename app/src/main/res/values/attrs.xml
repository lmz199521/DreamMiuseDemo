<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--声明样式-->
    <declare-styleable name="inputview">
        <!--name 表示引用时候的名字  format 表示接受什么样的文件  reference代表图片资源-->
        <attr name="input_icon" format="reference"></attr>
        <!--输入框的提示文字-->
        <attr name="input_hint" format="string"></attr>
        <!--输入内容是否以密文进行展示-->
        <attr name="is_password" format="boolean"></attr>
    </declare-styleable>

    <declare-styleable name="TTitleName">
        <attr name="titleNamm" format="string"></attr>
    </declare-styleable>
</resources>