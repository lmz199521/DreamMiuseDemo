<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
        <!--按钮被点击处于高亮状态下的亮度-->
     <item android:state_focused="true" android:drawable="@drawable/btn_login_h"></item>
          <!--按钮被按下的状态-->
    <item android:state_pressed="true" android:drawable="@drawable/btn_login_h"></item>
    <!--按钮被选中的状态-->
    <item android:drawable="@drawable/btn_login_h" android:state_selected="true"></item>
    <!--默认状态下-->
    <item android:drawable="@drawable/btn_login_n"></item>

</selector>